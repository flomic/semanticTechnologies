	rdfs:range xsd:string .

:has_language a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain :Book ;
	rdfs:range xsd:language .

:has_publication_year a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain :Book ;
	rdfs:range xsd:integer .

:has_title a owl:DatatypeProperty , owl:FunctionalProperty ;
	rdfs:domain :Book ;
	rdfs:range xsd:string .

:Author a owl:Class ;
	rdfs:subClassOf :Person .

:Book a owl:Class .

:Gender a owl:Class .

:Genre a owl:Class .

:Library a owl:Class .

:Person a owl:Class .

:Publisher a owl:Class .

:Reader a owl:Class ;
	rdfs:subClassOf :Person .


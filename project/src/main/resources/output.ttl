	rdfs:range :Author .

:has_gender a owl:ObjectProperty , owl:FunctionalProperty ;
	rdfs:domain :Person ;
	rdfs:range :Gender .

:has_genre a owl:ObjectProperty ;
	rdfs:domain :Book ;
